{
    "name": "iswai/devops",
    "type": "project",
    "description": "DevOps commands to make your life easier.",
    "keywords": [
        "DevOps",
        "GitHub",
        "pull requests"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Geert Eltink",
            "email": "geert.eltink@gmail.com"
        }
    ],
    "require": {
        "beberlei/assert": "^3.2",
        "ocramius/package-versions": "^1.5",
        "symfony/console": "^4.3",
        "symfony/process": "^4.3"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.4"
    },
    "config": {
        "sort-packages": true
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0.x-dev"
        }
    },
    "autoload": {
        "psr-4": {
            "Iswai\\DevOps\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "IswaiTest\\DevOps\\": "test/"
        }
    },
    "bin": [
        "bin/devops"
    ],
    "support": {
        "issues": "https://github.com/iswai/devops/issues",
        "source": "https://github.com/iswai/devops",
        "rss": "https://github.com/phly/iswai/devops/releases.atom"
    }
}
